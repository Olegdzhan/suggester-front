{
    "rules": {
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "react/jsx-uses-vars": 1,
        "react/jsx-uses-react": 1
    },
    "env": {
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}
